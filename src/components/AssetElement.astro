---
import getData from "@/src/components/getData";
import { AssetElement as ReactAssetElement } from "@stoati/tools";

const { path } = Astro.props;

const data = await getData();
---

<ReactAssetElement
  client:load
  data={data}
  path={path}
  alt="Photo de l'artiste"
  className="object-cover w-full h-full"
/>
